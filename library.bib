issn = {1460-2199},
journal = {Cereb. Cortex},
keywords = {connectome,environmental contribution,heritability,intrinsic connectivity network,twins},
month = {may},
number = {5},
pages = {2341--52},
pmid = {26891986},
publisher = {Oxford University Press},
title = {{Genetic and Environmental Contributions to Functional Connectivity Architecture of the Human Brain.}},
