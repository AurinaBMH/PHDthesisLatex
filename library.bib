file = {:Users/Aurina/Library/Application Support/Mendeley Desktop/Downloaded/Pletikos et al. - 2014 - Temporal specification and bilaterality of human neocortical topographic gene expression.pdf:pdf},
issn = {1097-4199},
journal = {Neuron},
keywords = {Adolescent,Adult,Age Factors,Aged,Analysis of Variance,Animals,Brene,Child,Developmental,Developmental: physiol,Female,Fetus,Functional Laterality,Functional Laterality: physiology,Gene Expression Profiling,Gene Expression Regulation,Gene Regulatory Networks,Humans,Infant,Macaca mulatta,Male,Middle Aged,Neocortex,Neocortex: enzymology,Neocortex: growth {\&} development,Neocortex: metabolism,Nerve Tissue Proteins,Nerve Tissue Proteins: genetics,Nerve Tissue Proteins: metabolism,Neurons,Neurons: metabolism,Newborn,Preschool,Principal Component Analysis,Species Specificity,Transcriptome,Young Adult},
