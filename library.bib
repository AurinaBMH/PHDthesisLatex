number = {2},
pages = {113--120},
pmid = {15240922},
publisher = {Hindawi},
title = {{Serial Analysis of Gene Expression: Applications in Human Studies.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15240922 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC548805},
volume = {2004},
year = {2004}
}
