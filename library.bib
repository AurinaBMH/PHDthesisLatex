pmid = {20534489},
title = {{High-resolution human genome structure by single-molecule analysis.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20534489 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2890719},
volume = {107},
year = {2010}
}
