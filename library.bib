issn = {1945-7928},
journal = {Proceedings. IEEE Int. Symp. Biomed. Imaging},
keywords = {development,high angular resolution diffusion imaging (HARDI),network analyses,rich club coefficient,structural connectivity,tractography},
pages = {624--627},
pmid = {24827471},
publisher = {NIH Public Access},
title = {{DEVELOPMENT OF THE RICH CLUB IN BRAIN CONNECTIVITY NETWORKS FROM 438 ADOLESCENTS {\&} ADULTS AGED 12 TO 30.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24827471 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4017916},
year = {2013}
}
@article{Pavlovic2014,
abstract = {Recently, there has been much interest in the community structure or mesoscale organization of complex networks. This structure is characterised either as a set of sparsely inter-connected modules or as a highly connected core with a sparsely connected periphery. However, it is often difficult to disambiguate these two types of mesoscale structure or, indeed, to summarise the full network in terms of the relationships between its mesoscale constituents. Here, we estimate a community structure with a stochastic blockmodel approach, the Erdős-R{\'{e}}nyi Mixture Model, and compare it to the much more widely used deterministic methods, such as the Louvain and Spectral algorithms. We used the Caenorhabditis elegans (C. elegans) nervous system (connectome) as a model system in which biological knowledge about each node or neuron can be used to validate the functional relevance of the communities obtained. The deterministic algorithms derived communities with 4-5 modules, defined by sparse inter-connectivity between all modules. In contrast, the stochastic Erdős-R{\'{e}}nyi Mixture Model estimated a community with 9 blocks or groups which comprised a similar set of modules but also included a clearly defined core, made of 2 small groups. We show that the "core-in-modules" decomposition of the worm brain network, estimated by the Erdős-R{\'{e}}nyi Mixture Model, is more compatible with prior biological knowledge about the C. elegans nervous system than the purely modular decomposition defined deterministically. We also show that the blockmodel can be used both to generate stochastic realisations (simulations) of the biological connectome, and to compress network into a small number of super-nodes and their connectivity. We expect that the Erdős-R{\'{e}}nyi Mixture Model may be useful for investigating the complex community structures in other (nervous) systems.},
annote = {Nodes are grouped according to a connection pattern and not anly connection density.

ER model: https://en.wikipedia.org/wiki/Erd{\%}C5{\%}91s{\%}E2{\%}80{\%}93R{\%}C3{\%}A9nyi{\_}model

279x279 undirected matrix. 2287 edges. (all synapses included.

Check if our hub neurons fall into any of those 9 blocks?},
author = {Pavlovic, Dragana M and V{\'{e}}rtes, Petra E and Bullmore, Edward T and Schafer, William R and Nichols, Thomas E},
doi = {10.1371/journal.pone.0097584},
file = {:Users/Aurina/Library/Application Support/Mendeley Desktop/Downloaded/Pavlovic et al. - 2014 - Stochastic blockmodeling of the modules and core of the Caenorhabditis elegans connectome.pdf:pdf},
issn = {1932-6203},
journal = {PLoS One},
keywords = {Algorithms,Animals,Biological,Caenorhabditis elegans,Caenorhabditis elegans: metabolism,Connectome,Models,Nerve Net,Nerve Net: metabolism,Neurons,Neurons: metabolism,Stochastic Processes},
month = {jan},
number = {7},
pages = {e97584},
pmid = {24988196},
publisher = {Public Library of Science},
title = {{Stochastic blockmodeling of the modules and core of the Caenorhabditis elegans connectome.}},
url = {http://journals.plos.org/plosone/article?id=10.1371/journal.pone.0097584},
volume = {9},
