pages = {17--36},
pmid = {23010748},
title = {{A Weighted and Directed Interareal Connectivity Matrix for Macaque Cerebral Cortex}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23010748 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3862262 https://academic.oup.com/cercor/article-lookup/doi/10.1093/cercor/bhs270},
volume = {24},
