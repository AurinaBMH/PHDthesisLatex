issn = {1460-2199},
journal = {Cereb. Cortex},
month = {nov},
number = {11},
pages = {4310--8},
pmid = {25596587},
title = {{Thalamocortical Connectivity Predicts Cognition in Children Born Preterm.}},
url = {http://cercor.oxfordjournals.org/content/25/11/4310.short?rss=1},
volume = {25},
year = {2015}
