eprint = {1608.08793},
file = {:Users/Aurina/Library/Application Support/Mendeley Desktop/Downloaded/Bentley et al. - 2016 - The Multilayer Connectome of Caenorhabditis elegans.pdf:pdf},
isbn = {1111111111},
issn = {15537358},
