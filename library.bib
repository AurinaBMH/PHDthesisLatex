pages = {e1004559},
pmid = {26636753},
publisher = {Public Library of Science},
title = {{Gene Expression Switching of Receptor Subunits in Human Brain Development.}},
url = {http://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1004559},
volume = {11},
year = {2015}
}
