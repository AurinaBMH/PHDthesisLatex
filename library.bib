month = {aug},
number = {8},
pages = {4204--4218},
pmid = {24677576},
title = {{Spatial patterns of genome-wide expression profiles reflect anatomic and fiber connectivity architecture of healthy human brain}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24677576 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4283562 http://doi.wiley.com/10.1002/hbm.22471},
