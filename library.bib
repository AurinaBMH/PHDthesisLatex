file = {:Users/Aurina/Library/Application Support/Mendeley Desktop/Downloaded/Mi{\v{s}}i{\'{c}} et al. - 2015 - Cooperative and Competitive Spreading Dynamics on the Human Connectome(2).pdf:pdf},
issn = {0896-6273},
journal = {Neuron},
