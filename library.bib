pages = {854--862},
publisher = {North-Holland},
title = {{Optimization of network robustness to random breakdowns}},
url = {https://www-sciencedirect-com.ezproxy.lib.monash.edu.au/science/article/pii/S0378437106002457{\#}fig2},
volume = {370},
