title = {{Age-related changes of gene expression in the neocortex: Preliminary data on RNA-Seq of the transcriptome in three functionally distinct cortical areas}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23062308 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3539811 http://www.journals.cambridge.org/abstract{\_}S0954579412000818},
