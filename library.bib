title = {{Replication of a Gene–Environment Interaction Via Multimodel Inference: Additive-Genetic Variance in Adolescents' General Cognitive Ability Increases with Family-of-Origin Socioeconomic Status}},
url = {https://search-proquest-com.ezproxy.lib.monash.edu.au/docview/1659893952?OpenUrlRefId=info:xri/sid:primo{\&}accountid=12528}
}
