@article{Howie2012,
abstract = {The 1000 Genomes Project and disease-specific sequencing efforts are producing large collections of haplotypes that can be used as reference panels for genotype imputation in genome-wide association studies (GWAS). However, imputing from large reference panels with existing methods imposes a high computational burden. We introduce a strategy called 'pre-phasing' that maintains the accuracy of leading methods while reducing computational costs. We first statistically estimate the haplotypes for each individual within the GWAS sample (pre-phasing) and then impute missing genotypes into these estimated haplotypes. This reduces the computational cost because (i) the GWAS samples must be phased only once, whereas standard methods would implicitly repeat phasing with each reference panel update, and (ii) it is much faster to match a phased GWAS haplotype to one reference haplotype than to match two unphased GWAS genotypes to a pair of reference haplotypes. We implemented our approach in the MaCH and IMPUTE2 frameworks, and we tested it on data sets from the Wellcome Trust Case Control Consortium 2 (WTCCC2), the Genetic Association Information Network (GAIN), the Women's Health Initiative (WHI) and the 1000 Genomes Project. This strategy will be particularly valuable for repeated imputation as reference panels evolve.},
author = {Howie, Bryan and Fuchsberger, Christian and Stephens, Matthew and Marchini, Jonathan and Abecasis, Gon{\c{c}}alo R},
doi = {10.1038/ng.2354},
file = {:Users/Aurina/Library/Application Support/Mendeley Desktop/Downloaded/Howie et al. - 2012 - Fast and accurate genotype imputation in genome-wide association studies through pre-phasing.pdf:pdf},
issn = {1546-1718},
journal = {Nat. Genet.},
month = {jul},
number = {8},
pages = {955--9},
pmid = {22820512},
publisher = {NIH Public Access},
title = {{Fast and accurate genotype imputation in genome-wide association studies through pre-phasing.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22820512 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3696580},
volume = {44},
year = {2012}
}
@article{Smith2012,
abstract = {Diffusion MRI streamlines tractography suffers from a number of inherent limitations, one of which is the accurate determination of when streamlines should be terminated. Use of an accurate streamlines propagation mask from segmentation of an anatomical image confines the streamlines to the volume of the brain white matter, but does not take full advantage of all of the information available from such an image. We present a modular addition to streamlines tractography, which makes more effective use of the information available from anatomical image segmentation, and the known properties of the neuronal axons being reconstructed, to apply biologically realistic priors to the streamlines generated; we refer to this as "Anatomically-Constrained Tractography". Results indicate that some of the known false positives associated with tractography algorithms are prevented, such that the biological accuracy of the reconstructions should be improved, provided that state-of-the-art streamlines tractography methods are used.},
author = {Smith, Robert E and Tournier, Jacques-Donald and Calamante, Fernando and Connelly, Alan},
doi = {10.1016/j.neuroimage.2012.06.005},
file = {:Users/Aurina/Library/Application Support/Mendeley Desktop/Downloaded/Smith et al. - 2012 - Anatomically-constrained tractography improved diffusion MRI streamlines tractography through effective use of ana.pdf:pdf},
issn = {1095-9572},
journal = {Neuroimage},
keywords = {Brain,Brain Mapping,Brain Mapping: methods,Brain: anatomy {\&} histology,Computer-Assisted,Computer-Assisted: methods,Diffusion Tensor Imaging,Diffusion Tensor Imaging: methods,Humans,Image Interpretation},
month = {sep},
number = {3},
pages = {1924--38},
pmid = {22705374},
title = {{Anatomically-constrained tractography: improved diffusion MRI streamlines tractography through effective use of anatomical information.}},
