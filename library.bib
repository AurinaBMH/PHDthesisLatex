pages = {139--149},
pmid = {29203898},
title = {{An interactive framework for whole-brain maps at cellular resolution}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/29203898 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5994773 http://www.nature.com/articles/s41593-017-0027-7},
