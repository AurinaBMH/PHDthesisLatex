@article{Fraser2005,
abstract = {It is well established that gene expression levels in many organisms change during the aging process, and the advent of DNA microarrays has allowed genome-wide patterns of transcriptional changes associated with aging to be studied in both model organisms and various human tissues. Understanding the effects of aging on gene expression in the human brain is of particular interest, because of its relation to both normal and pathological neurodegeneration. Here we show that human cerebral cortex, human cerebellum, and chimpanzee cortex each undergo different patterns of age-related gene expression alterations. In humans, many more genes undergo consistent expression changes in the cortex than in the cerebellum; in chimpanzees, many genes change expression with age in cortex, but the pattern of changes in expression bears almost no resemblance to that of human cortex. These results demonstrate the diversity of aging patterns present within the human brain, as well as how rapidly genome-wide patterns of aging can evolve between species; they may also have implications for the oxidative free radical theory of aging, and help to improve our understanding of human neurodegenerative diseases.},
author = {Fraser, Hunter B and Khaitovich, Philipp and Plotkin, Joshua B and P{\"{a}}{\"{a}}bo, Svante and Eisen, Michael B},
doi = {10.1371/journal.pbio.0030274},
file = {:Users/Aurina/Library/Application Support/Mendeley Desktop/Downloaded/Fraser et al. - 2005 - Aging and gene expression in the primate brain.pdf:pdf},
issn = {1545-7885},
journal = {PLoS Biol.},
month = {sep},
number = {9},
pages = {e274},
pmid = {16048372},
publisher = {Public Library of Science},
title = {{Aging and gene expression in the primate brain.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16048372 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC1181540},
