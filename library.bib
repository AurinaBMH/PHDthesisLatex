title = {{Diffusion tensor imaging of the brain.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2041910{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {4},
year = {2007}
}
