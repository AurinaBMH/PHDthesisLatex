volume = {67},
year = {2013}
}
@article{Gushchin2015,
abstract = {Using the most recent data on the connectivity of the C. elegans neural network, we find optimal two-dimensional positions of interneurons that minimize the total wiring length provided that the positions of motor and sensory neurons are fixed. The rationale behind fixing motor and sensory neurons is the following: while positions of motor and sensory neurons can be influenced by the locations of muscles and sensory organs they are attached to, the main function of interneurons is to connect other neurons, and their placement could try to minimize the wiring length. Solutions for l1, l2 and squared l2-norm were obtained. For the Euclidean norm l2, the relative and absolute difference between the real and optimal total wiring lengths is minimal among these functions of distance. Additional network constraints were discussed such as assignment of different weights to electrical or chemical connections, fixation of "tail" interneurons, minimal interneural distance limitation, and others. These constraints were compared by their influence on the optimal positions of interneurons.},
author = {Gushchin, Andrey and Tang, Ao},
doi = {10.1371/journal.pone.0145029},
file = {:Users/Aurina/GoogleDrive/Genetics{\_}connectome/Literature/Guschin - Constrained optimization in Celegans.PDF:PDF},
issn = {19326203},
journal = {PLoS One},
number = {12},
pages = {1--16},
