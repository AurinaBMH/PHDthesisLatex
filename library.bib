pmid = {25368179},
title = {{Anatomical accuracy of brain connections derived from diffusion MRI tractography is inherently limited}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25368179 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4246325 http://www.pnas.org/lookup/doi/10.1073/pnas.1405672111},
volume = {111},
