author = {Heuvel, Martijn P Van Den and Kahn, Ren{\'{e}} S and Go{\~{n}}i, Joaqu{\'{i}}n and Sporns, Olaf and van den Heuvel, Martijn P},
doi = {10.1073/pnas.1203593109/-/DCSupplemental.www.pnas.org/cgi/doi/10.1073/pnas.1203593109},
file = {:Users/Aurina/Library/Application Support/Mendeley Desktop/Downloaded/Heuvel et al. - 2012 - Brain Communication.pdf:pdf;:Users/Aurina/Library/Application Support/Mendeley Desktop/Downloaded/Heuvel et al. - 2012 - Brain Communication(2).pdf:pdf},
