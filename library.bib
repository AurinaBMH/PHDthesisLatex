title = {{Regional patterns of gene expression in human and chimpanzee brains.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15289471 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC509255},
volume = {14},
