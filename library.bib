pages = {910--913},
pmid = {11988575},
title = {{Specificity and Stability in Topology of Protein Networks}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11988575 http://www.sciencemag.org/cgi/doi/10.1126/science.1065103},
volume = {296},
year = {2002}
}
