journal = {eneuro},
keywords = {RNA sequencing,cell type,gene expression,marker gene,microarray},
number = {6},
pages = {ENEURO.0212--17.2017},
pmid = {29204516},
title = {{Cross-Laboratory Analysis of Brain Cell Type Transcriptomes with Applications to Interpretation of Bulk Tissue Data}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/29204516 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5707795 http://eneuro.sfn.org/lookup/doi/10.1523/ENEURO.0212-17.2017},
