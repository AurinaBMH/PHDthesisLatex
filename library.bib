file = {:Users/Aurina/Library/Application Support/Mendeley Desktop/Downloaded/Sato et al. - 2016 - Connectome hubs at resting state in children and adolescents Reproducibility and psychopathological correlation.pdf:pdf},
issn = {1878-9293},
