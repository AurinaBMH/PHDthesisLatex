issn = {2045-2322},
journal = {Sci. Rep.},
month = {mar},
pages = {547--579},
publisher = {Nature Publishing Group},
title = {{Consensus clustering in complex networks}},
url = {http://www.nature.com/articles/srep00336 http://arxiv.org/abs/1203.6093 http://dx.doi.org/10.1038/srep00336},
volume = {2},
