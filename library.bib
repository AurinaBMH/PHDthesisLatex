volume = {152},
year = {2013}
}
@article{Huang2016,
abstract = {Classical quantitative genetic analyses estimate additive and non-additive genetic and 17 environmental components of variance from phenotypes of related individuals. The genetic 18 variance components are defined in terms of genotypic values reflecting underlying genetic 19 architecture (additive, dominance and epistatic genotypic effects) and allele frequencies. 20 However, the dependency of the definition of genetic variance components on the 21 underlying genetic models is not often appreciated. Here, we show how the partitioning of 22 additive and non-additive genetic variation is affected by the genetic models and 23 parameterization of allelic effects. We show that arbitrarily defined variance components 24 often capture a substantial fraction of total genetic variation regardless of the underlying 25 genetic architecture in simulated and real data. Therefore, variance component analysis 26 cannot be used to infer genetic architecture of quantitative traits. The genetic basis of 27 quantitative trait variation in a natural population can only be defined empirically using 28 high resolution mapping methods followed by detailed characterization of QTL effects},
