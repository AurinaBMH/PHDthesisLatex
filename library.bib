title = {{Analysis of Gene Expression Profiles in the Human Brain Stem, Cerebellum and Cerebral Cortex.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/27434030 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4951119},
