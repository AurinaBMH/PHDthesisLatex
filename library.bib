doi = {10.1371/journal.pcbi.1001066},
file = {:Users/Aurina/Library/Application Support/Mendeley Desktop/Downloaded/Varshney et al. - 2011 - Structural properties of the Caenorhabditis elegans neuronal network.pdf:pdf},
issn = {1553-7358},
