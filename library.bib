title = {{The Human Connectome Project: A data acquisition perspective}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22366334 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3606888 https://linkinghub.elsevier.com/retrieve/pii/S1053811912001954},
volume = {62},
