journal = {PLoS Comput. Biol.},
keywords = {Algorithms,Animals,Brain,Brain Mapping,Brain: physiology,Cerebral Cortex,Cerebral Cortex: physiology,Computer Simulation,Macaca,Models, Neurological,Nerve Net,Neural Pathways,Signal Transduction},
month = {jan},
number = {1},
pages = {e1003427},
pmid = {24415931},
publisher = {Public Library of Science},
title = {{Communication efficiency and congestion of signal traffic in large-scale brain networks.}},
